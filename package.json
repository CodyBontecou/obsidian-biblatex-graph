{
  "name": "obsidian-biblatex-graph",
  "version": "1.0.0",
  "description": "Turn BibLaTeX @citekey references into clickable links and graph nodes in Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production"
  },
  "keywords": [
    "obsidian",
    "biblatex",
    "citation",
    "graph"
  ],
  "author": "Cody Bontecou",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.20.0",
    "obsidian": "latest",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  }
}
